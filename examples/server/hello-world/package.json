    "main": "index.server.js",
    "engines": {
        "node": ">=10.x",
        "npm": ">=6.x"
    },
    "dependencies": {
        "@material-ui/core": "^4.9.0",
        "@material-ui/icons": "^4.5.1",
        "hyperflow": "file:../../../hyperflow-0.3.0.tgz",
