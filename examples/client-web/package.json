{
  "name": "hyperflow-client-web-examples",
  "description": "Example client apps built using Hyperflow.",
  "authors": [
    "Tuan Le <tuan.t.lei@gmail.com> (https://github.com/tuantle)"
  ],
  "private": true,
  "homepage": "https://github.com/tuantle/hyperflow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tuantle/hyperflow/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "stage-2",
      "stage-3",
      "react"
    ],
    "plugins": [
      "transform-strict-mode"
    ]
  },
  "jspm": {
    "dependencies": {
      "material-ui": "npm:material-ui@^0.15.4",
      "moment": "npm:moment@^2.15.0",
      "react": "npm:react@^15.3.1",
      "react-dom": "npm:react-dom@^15.3.1",
      "react-router": "npm:react-router@^2.8.0",
      "react-tap-event-plugin": "npm:react-tap-event-plugin@^1.0.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "dependencies": {
    "hyperflow": "latest",
    "material-ui": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-router": "latest",
    "react-tap-event-plugin": "latest",
    "moment": "latest"
  },
  "devDependencies": {
    "babel-loader": "latest",
    "babel-core": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-stage-0": "latest",
    "babel-preset-stage-1": "latest",
    "babel-preset-stage-2": "latest",
    "babel-preset-stage-3": "latest",
    "babel-plugin-transform-strict-mode": "latest",
    "babel-preset-react": "latest",
    "babel-eslint": "latest",
    "eslint": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-native": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tuantle/hyperflow.git"
  },
  "scripts": {
    "webpack-build-counter": "NODE_ENV=development webpack --progress --colors --config counter-webpack.config.js",
    "webpack-start-counter": "NODE_ENV=development webpack-dev-server --content-base --progress --colors counter/build",
    "jspm-start-counter": "NODE_ENV=development live-server --no-browser -q --open=counter/index.html",
    "jspm-start-calculator": "NODE_ENV=development live-server --no-browser -q --open=calculator/index.html"
  },
  "modulePathIgnorePatterns": [
    "/node_modules/",
    "/jspm_packages/"
  ]
}
